        "AlertMask": {
            "name": "MAX31760AlertMask",
            "description": "Enable alert mask",
            "tip": "Enable selected alarm conditions",
            "level": 0,
            "type": "checkbox",
            "checkedValue": true,
            "uncheckedValue": false,
            "onChange": "AlertMaskCf",
            "children": {
               "1": [
                  "AlertMaskLocalTempHigh",
                  "AlertMaskLocalOverTemp",
